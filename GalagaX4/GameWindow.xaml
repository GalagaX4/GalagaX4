<Window x:Class="GalagaX4.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GalagaX4"
        mc:Ignorable="d"
        Title="GameWindow" Height="650" Width="860" Background="Black">
    <Grid x:Name="MyGrid" KeyDown="MyGrid_KeyDown">
        <Canvas x:Name="lv1Canvas" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        <Canvas x:Name="lv2Canvas" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
    </Grid>
</Window>
