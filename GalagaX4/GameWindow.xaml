<Window x:Class="GalagaX4.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GalagaX4"
        mc:Ignorable="d"
        Title="Galaga X4" Height="650" Width="860" Background="Black" Loaded="Page_Loaded" Icon="pics/galaga_ship.ico">
    <Grid x:Name="MyGrid" KeyDown="MyGrid_KeyDown">
        <Canvas x:Name="canvas" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >
            <Label x:Name="label" Content="Label" Canvas.Left="674" Canvas.Top="354" Foreground="#FFF9F1F1" Height="28" Width="118"/>
        </Canvas>
        <MediaElement x:Name="mediaElement" HorizontalAlignment="Left" Height="14" Margin="835,10,0,0" VerticalAlignment="Top" Width="9"
                      UnloadedBehavior="Manual" MediaEnded="Element_MediaEnded" Source="main2.wav"/>
        <ProgressBar x:Name="progressBar" Minimum="0" Maximum="10" HorizontalAlignment="Left" Height="31" VerticalAlignment="Top" Width="411" Margin="10,579,0,0" Foreground="#FF0023FF"/>
    </Grid>
</Window>
